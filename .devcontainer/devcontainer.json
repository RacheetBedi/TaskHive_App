{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [3000],
  "otherPortsAttributes": {
    "onAutoForward": "ignore"
  },
  "name": "Flutter",
  "customizations": {
    "vscode": {
      "extensions": [
        "Dart-Code.dart-code",
        "Dart-Code.flutter"
      ],
      "settings": {
        "remote.autoForwardPorts": false,
        "remote.restoreForwardedPorts": false,
      }
    }
  }
}
